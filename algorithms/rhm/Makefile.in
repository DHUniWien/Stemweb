

pkglib_LTLIBRARIES = binarysankoff.so
binarysankoff_so_LDFLAGS = -module -avoid-version -shared


binarysankoff:	
	gcc -lz binarysankoff.c -o binarysankoff

NCD:
	gcc -lz NCD.c -o NCD
	
rhm_osx:
	gcc -dynamiclib -Wall -I/usr/include/python2.7/ -lpython2.7 -lz -o binarysankoff.dylib binarysankoff.c 
	mv ./binarysankoff.dylib ./binarysankoff.so

binarysankoff_python:
	gcc -shared -I/usr/include/python2.7/ -lpython2.7 -lz -o binarysankoff.so binarysankoff.c
	
	
